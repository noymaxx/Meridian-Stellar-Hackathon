# Just copy and past in .env file
# ------------------------
# Stellar Network Configuration
# Use 'testnet' for development, 'mainnet' for production
VITE_STELLAR_NETWORK=testnet

# Stellar Horizon URLs
VITE_STELLAR_HORIZON_URL_TESTNET=https://horizon-testnet.stellar.org
VITE_STELLAR_HORIZON_URL_MAINNET=https://horizon.stellar.org

# Soroban RPC URLs  
VITE_STELLAR_SOROBAN_RPC_URL_TESTNET=https://soroban-testnet.stellar.org
VITE_STELLAR_SOROBAN_RPC_URL_MAINNET=https://soroban-mainnet.stellar.org

# Network Passphrases
VITE_STELLAR_NETWORK_PASSPHRASE_TESTNET=Test SDF Network ; September 2015
VITE_STELLAR_NETWORK_PASSPHRASE_MAINNET=Public Global Stellar Network ; September 2015

# Application Configuration
VITE_APP_NAME=RWA Lending Protocol
VITE_APP_VERSION=1.0.0